PREFIX dbp: <http://dbpedia.org/property/>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
Prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
Prefix dbpediaowl: <http://dbpedia.org/ontology/>

SELECT (COUNT(?laureat) AS ?number) ?country
WHERE {
  ?laureat dbo:birthPlace ?country .
  ?country a dbpediaowl:Country .
  
} 
Group by ?country



PREFIX dct: <http://purl.org/dc/terms/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX db: <http://dbpedia.org/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX nobel: <http://data.nobelprize.org/terms/>
PREFIX dbp: <http://dbpedia.org/property/>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
Prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
Prefix dbpediaowl: <http://dbpedia.org/ontology/>

SELECT ?name ?institution ?countryName ?country ?subclass ?continent
WHERE{
  	?laureate a nobel:Laureate ;
     	dbpediaowl:affiliation ?institution;
        dbo:birthPlace ?country;
        foaf:givenName ?name .
  	
    ?country a dbpediaowl:Country ;
              owl:sameAs ?subclass ;
              rdfs:label ?countryName.
  	OPTIONAL {
    	?subclass a <http://dbpedia.org/ontology/PopulatedPlace>;
              dbo:continent ?continent .
    
  }
}
